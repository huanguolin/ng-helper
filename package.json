    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "typescript": "~5.4.5"
