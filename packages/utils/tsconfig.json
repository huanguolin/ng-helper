{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2022",
		"lib": ["ES2022"],
		"noEmit": true,
		"rootDir": ".",
		"strict": true,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"exclude": [
	  "node_modules", "babel.config.js", "jest.config.ts"
	]
}
