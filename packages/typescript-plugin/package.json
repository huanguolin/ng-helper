    "@types/jest": "^29.5.12",
    "@types/node": "18.x",
    "@types/vscode": "^1.90.0",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "esbuild": "^0.21.4",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "typescript": "~5.4.5"
  }
